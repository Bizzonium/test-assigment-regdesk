{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Lib",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "esnext",
    "useDefineForClassFields": true,
    "module": "CommonJS",
    "lib": ["esnext", "DOM", "DOM.Iterable"],
    "moduleResolution": "node",
    "noEmit": false,
    "allowImportingTsExtensions": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": ["src"]
}
