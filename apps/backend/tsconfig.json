{
  "extends": "@azizonkg/tsconfig/nestjs.json",
  "compilerOptions": {
    "strictNullChecks": true,
    "noImplicitAny": true,
    "strictBindCallApply": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "@/*": ["./*"]
    },
    "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json"
  },
  "include": ["src/**/*", "test/**/*"]
}
